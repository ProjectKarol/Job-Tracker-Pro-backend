{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "args": [
        "${workspaceFolder}/jtpbackend/src/main.ts"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/jtpbackend/.env",
      "cwd": "${workspaceRoot}/jtpbackend",
      "console": "integratedTerminal",
      "protocol": "inspector"
    },
  ]
}